==

SELECT a1.z
FROM a AS a1 JOIN b AS b1 ON a1.w = b1.w
             JOIN c AS c1 ON a1.x = c1.x
             JOIN d AS d1 ON a1.y = d1.y
WHERE a1.x = b1.x
  AND a1.y = c1.y
  AND a1.z = d1.z;
