SELECT i + 1 AS result
FROM generate_series(1, 5) AS gs(i)
INTERSECT
SELECT i * 2 AS result
FROM generate_series(1, 5) AS gs(i)
--
+-----------+
|result     |
+-----------+
|          2|
|          4|
|          6|
+-----------+
==

